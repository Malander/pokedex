{
  "name": "telepass-code-challenge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port=5173 --open",
    "build": "tsc && vite build",
    "preview": "vite build && vite preview --open",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
